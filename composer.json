{
  "name": "wyrihaximus/tactician-job-command-mapper",
  "description": "Job to Command mapper for Tactician",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "doctrine/annotations": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\Tactician\\JobCommand\\": "src/"
    },
    "files": [
      "src/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\Tactician\\JobCommand\\": "tests/",
      "Test\\App\\": "test-app/"
    }
  },
  "config": {
    "platform": {
      "php": "7.0"
    }
  }
}
